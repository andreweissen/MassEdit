{
  "name": "massedit",
  "version": "3.1.1",
  "author": "Andrew Eissen",
  "license": "CC-BY-SA-3.0",
  "description": "A task automation and bulk editing script",
  "main": "dev.py",
  "scripts": {
    "jshint": "jshint src/code.js",
    "doc": "jsdoc -p -c jsdoc.json",
    "pushcode": "python dev.py push https://dev.fandom.com/wiki/MediaWiki:MassEdit/code.js src/code.js",
    "pushi18n": "python dev.py push https://dev.fandom.com/wiki/MediaWiki:Custom-MassEdit/i18n.json src/data/i18n.json",
    "pullcode": "python dev.py pull https://dev.fandom.com/wiki/MediaWiki:MassEdit/code.js src/code.js",
    "pulli18n": "python dev.py pull https://dev.fandom.com/wiki/MediaWiki:Custom-MassEdit/i18n.json src/data/i18n.json"
  },
  "directories": {
    "doc": "doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreweissen/MassEdit.git"
  },
  "bugs": {
    "url": "https://github.com/andreweissen/MassEdit/issues"
  },
  "homepage": "https://github.com/andreweissen/MassEdit#readme",
  "dependencies": {
    "python": "0.0.4"
  },
  "devDependencies": {
    "jsdoc": "^3.6.7",
    "jshint": "^2.13.1"
  }
}
